// write a program to check weather  number is prime or a composite number.
#include<stdio.h>
int main(){
    int n;
    printf("Enter the number:");
    scanf("%d",&n);
    int a=0;
    for(int i=2;i<=n;i++){
        if(n%i==0){
            a=1;
            break;
        }
    }
    if(a==0){
        printf(" %d is a prime number!",n);
    }
    else{
        printf("Your number is composite number ");
    }
    return 0;
} 
