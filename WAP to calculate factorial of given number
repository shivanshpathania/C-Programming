// write a code to calculate factorial of given number entered by user
#include<stdio.h>
int main(){
    int n;
    printf("Enter the number :");
    scanf("%d",&n);
    int fact = 1;
    for(int i=1;i<=n;i++){ // loop will run when n = i
        fact = fact * i;  
    }
    printf("The factorial of %d is :%d",n,fact);
        
    return 0;
}
